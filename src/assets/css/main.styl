.fade-enter-active, .fade-leave-active {
  transition: opacity 0.3s ease;
}

.fade-enter-from {
  opacity: 0;
}
.fade-leave-to {
  opacity: 0;
}

.slide-fade-enter-active {
  transition: all 0.3s ease-out;
}

.slide-fade-leave-active {
  transition: all 0.3s
}

.slide-fade-enter-from {
  transform: translateX(10vw);
  opacity: 0.5;
}
.slide-fade-leave-to {
  transform: translateX(-10vw);
  opacity: 0.5;
}


