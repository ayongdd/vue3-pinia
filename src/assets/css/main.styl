.chat_wrap {
  padding 54px 10px 60px 10px
  height 100vh
  overflow hidden
  overflow-y auto
  .notice_div {
    position absolute
    background-color #ffd01e
    width 100%
    top:45px
    left 0
    padding 5px 10px
    font-size 14px
    color red;
    display flex
    justify-content space-between
    align-items center
    z-index 999
    ._l {
      display flex
      align-items center
      flex:1
      marquee  {
        flex:1
      }
    }
    i {
      font-size 20px
    }
  }
  &.msg_log {
    padding-bottom 4px
  }
  .chat_item {
    width 100%
    text-align center
    margin-bottom 10px
    .time {
      display inline-block
      text-align center
      background-color #c6c4c4
      border-radius 10px
      color #ffffff
      font-size 12px
      padding 3px 5px
      margin-top 15px
      margin-bottom 10px
    }
    .chat_content {
      display flex
      align-items start
      justify-content revert
      &.left {
        direction: ltr;
        >img {
          margin-right 15px
        }
        .text_content {
          background-color #ffffff
          color #333
          text-align right
          &.card {
            padding 2px
            width 200px
          }
          .card_top {
            display flex
            justify-content flex-start
            align-items center
            direction ltr
            background-color #ffd01e
            padding 7px 10px
            >img {
              width 40px
              height 40px
              border-radius 5px
              margin-right 10px
            }
            >p {
              color #000
            }
          }
          >p {
            color #000
            text-align left
            background-color #d5d2d2
            padding 5px 10px
            font-size 14px
          }
          .audio_wrap {
            direction: rtl
            display flex
            justify-content space-between
          }
          .crlc {
            font-size: 30px;
            color: red;
            position: absolute;
            right: -20px;top:12px;
            width: 8px;
            height: 8px;
            background-color: red;
            border-radius: 10px;
          }
          &::after {
            border: 5px solid transparent;
            border-right: 10px solid #ffffff;
            position absolute
            left -14px
            top:5px
          }
          &.redPack {
            &::after {
              border-right: 10px solid #fa812b;
            }
          }
          &.video {
            background-color #333
            &::after {
              border-right: 10px solid #333;
            }
          }
        }
      }
      &.right {
        /*justify-content flex-end*/
        direction: rtl;
        >img {
          margin-left 15px
        }
        .text_content  {
          background-color #1989fa
          color #ffffff
          text-align left
          &.card {
            padding 2px
            width 200px
          }
          .card_top {
            display flex
            justify-content flex-start
            align-items center
            direction ltr
            background-color #ffd01e
            padding 7px 10px
            >img {
              width 40px
              height 40px
              border-radius 5px
              margin-right 10px
            }
            >p {
              color #000
            }
          }
          >p {
            color #000
            background-color #d5d2d2
            padding 5px 10px
            font-size 14px
          }
          .audio_wrap {
            direction: ltr
            display flex
            justify-content space-between
          }
          &::after {
            border: 5px solid transparent;
            border-left: 10px solid #1989fa;
            position absolute
            right -13px
            top:5px
          }
          &.redPack {
            &::after {
              border-left: 10px solid #fa812b;
            }
          }
          &.video {
            background-color #333
            &::after {
              border-left: 10px solid #333;
            }
          }

        }
      }
      .notice {
        width 100%
        text-align center
        font-size 12px
        >span {
          display inline-block
          color #ffffff
          background-color rgba(0,0,0,.2)
          padding 4px 8px
          border-radius 10px
          line-height 1.3
        }
      }
      >.avatar {
        width 40px
        height 40px
        border-radius 7px
      }
      .text_content {
        background #fff
        padding 9px
        border-radius 5px
        position relative
        max-width 52vw
        word-break break-all
        .emoji {
          vertical-align: sub;
          width: 26px;
        }
        &::after {
          content ""
          width 0
          height 0
        }
        &.redPack {
          padding 0
          border-radius 8px
          position relative
          width 210px
          /*overflow hidden*/

          .top_wraps {
            background-color #fa812b
            display flex
            justify-content flex-start
            direction ltr
            padding 10px
            align-items center
            border-radius 10px 10px 0 0;
            >i {
              display block
              font-size 28px
              color #f6eec9;
              font-weight 100
              margin-right 10px
            }
            ._red {
              text-align left
              font-weight 100
              font-size 14px
              color #ffffff
              p:last-child {
                margin-top 5px
                font-size 12px
              }
            }
          }
          .bot_text {
            font-size 12px
            text-align left
            padding 7px 0
            padding-left 10px
            color #666
            background: #fff;
            border-radius: 0 0 5px 5px;
          }
        }
        &.video{
          .play_icon {
            font-size 40px
            line-height 130px
            color #ffffff
            position: absolute;
            right: 50%;
            margin-right: -20px;
          }
        }
      }
    }
  }
